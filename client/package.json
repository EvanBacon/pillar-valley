{
  "name": "pillar-valley",
  "version": "3.0.0",
  "author": "Evan Bacon",
  "private": true,
  "scripts": {
    "metadata": "fastlane deliver --skip_binary_upload",
    "fix": "rm -rf node_modules/gl-matrix/.babelrc",
    "test": "node_modules/.bin/jest",
    "eject": "react-scripts eject",
    "lint": "eslint --ignore-path .gitignore .",
    "lint:fix": "eslint --ignore-path .gitignore . --fix",
    "prettier": "prettier --write '*.js'",
    "format-code": "yarn run prettier && yarn run lint:fix",
    "predeploy": "npm run build",
    "licenses": "npx npm-license-crawler -onlyDirectDependencies -dependencies -json src/constants/Licenses.json",
    "deploy": "gh-pages -d public",
    "deploy:net": "netlify deploy --dir web-build --prod"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/react-native-action-sheet": "^3.5.0",
    "@expo/vector-icons": "10.0.6",
    "@react-native-community/masked-view": "0.1.6",
    "@react-navigation/bottom-tabs": "^5.5.2",
    "@react-navigation/drawer": "^5.7.5",
    "@react-navigation/material-top-tabs": "^5.2.10",
    "@react-navigation/native": "^5.4.0",
    "@react-navigation/stack": "^5.5.1",
    "@rematch/core": "^0.6.0",
    "@rematch/persist": "^0.2.1",
    "expo": "^37.0.2",
    "expo-av": "~8.1.0",
    "expo-constants": "9.0.0",
    "expo-facebook": "~8.1.0",
    "expo-firebase-analytics": "^2.3.0",
    "expo-gl": "~8.1.0",
    "expo-graphics": "^1.1.0",
    "expo-haptics": "~8.1.0",
    "expo-image-picker": "~8.1.0",
    "expo-sensors": "~8.1.0",
    "expo-store-review": "~2.1.0",
    "expo-three": "5.4.0",
    "firebase": "^7.15.0",
    "gsap": "^2.0.1",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-37.0.1.tar.gz",
    "react-native-animatable": "^1.3.3",
    "react-native-gesture-handler": "~1.6.0",
    "react-native-reanimated": "~1.7.0",
    "react-native-safe-area-context": "0.7.3",
    "react-native-screens": "~2.2.0",
    "react-native-tab-view": "^2.14.4",
    "react-native-web": "~0.11.2",
    "react-redux": "^5.0.6",
    "redux-persist": "^5.6.9",
    "three": "^0.113.2",
    "three.proton.js": "^0.1.5"
  },
  "devDependencies": {
    "@babel/core": "7.9.0",
    "babel-eslint": "10.0.3",
    "gh-pages": "^1.2.0"
  }
